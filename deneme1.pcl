class Araba:
    def __init__(self, marka, model):
        self.marka = marka
        self.model = model

    def bilgileri_goster(self):
        print("Marka: " + self.marka + ", Model: " + self.model)

araba1 = Araba("Toyota", "Corolla")
araba1.bilgileri_goster()  # Output: Marka: Toyota, Model: Corolla