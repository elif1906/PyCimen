import sys
import time


class colors:
    WHITE = '\033[32m'  
    BOLD = '\033[1m'
    END = '\033[0m'


def print_with_typing(text, delay=0.05):
    for char in text:
        sys.stdout.write(char)
        sys.stdout.flush()
        time.sleep(delay)



print(colors.BOLD + "Hello, " + colors.END, end="")
print_with_typing(colors.BOLD + "PyCimen" + colors.END, delay=0.1)
print(colors.WHITE + " Language!" + colors.END)

